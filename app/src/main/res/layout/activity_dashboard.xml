<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mainbg"
    tools:context=".Dashboard">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/alfa_slab_one"
        android:text="TRAVEL WITH US"
        android:textColor="@color/white"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/logo_logout"
        android:layout_width="34dp"
        android:layout_height="34dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logut_4"
        android:layout_marginLeft="340dp"
        android:layout_marginTop="20dp"
        android:onClick="Logout"/>

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/search_bg"
        android:drawableRight="@drawable/ic_baseline_search_24"
        android:ems="10"
        android:hint="Search Places"
        android:textColor="@color/black"
        android:inputType="textPersonName"
        android:padding="10dp"
        android:paddingStart="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView1" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/ramabhadra"
        android:text="Most Visited"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="407dp"
        android:layout_height="564dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/amarante"
                android:paddingBottom="10dp"
                android:text="Maidan (Garer Math)"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="383dp"
                android:layout_height="200dp"
                android:paddingLeft="25sp"
                app:srcCompat="@drawable/places1" />

            <Button
                android:id="@+id/button1"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:text="Know More" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/amarante"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="Park Street"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="383dp"
                android:layout_height="200dp"
                app:srcCompat="@drawable/places2" />

            <Button
                android:id="@+id/button3"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:text="KNOW MORE" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/amarante"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="Victoria Memorial"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="383dp"
                android:layout_height="200dp"
                app:srcCompat="@drawable/places" />

            <Button
                android:id="@+id/button4"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:text="KNOW MORE" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/amarante"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="St. Paul's Cathedral"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="383dp"
                android:layout_height="200dp"
                app:srcCompat="@drawable/topplace" />

            <Button
                android:id="@+id/button5"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:text="KNOW MORE" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/amarante"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="Iconic Howrah Bridge"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="383dp"
                android:layout_height="200dp"
                app:srcCompat="@drawable/howrabridge" />

            <Button
                android:id="@+id/button6"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:text="KNOW MORE" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/amarante"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="Princep Ghat"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="383dp"
                android:layout_height="200dp"
                app:srcCompat="@drawable/princepghat" />

            <Button
                android:id="@+id/button7"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:text="KNOW MORE" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/amarante"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="National Library"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="18sp" />


            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="214dp"
                app:srcCompat="@drawable/calcutta_lib" />

            <Button
                android:id="@+id/button8"
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:text="KNOW MORE" />


        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>